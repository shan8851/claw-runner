[Desktop Entry]
Name=claw-runner
Comment=KRunner actions for Clawdbot
Type=Service
Icon=applications-internet
X-KDE-ServiceTypes=Plasma/Runner
X-KDE-PluginInfo-Name=ai.openclaw.ClawRunner
X-KDE-PluginInfo-Author=shan8851
X-KDE-PluginInfo-Email=
X-KDE-PluginInfo-Version=0.1.0
X-KDE-PluginInfo-License=MIT
X-KDE-PluginInfo-EnabledByDefault=true

# DBus runner wiring
X-KDE-Runner-DBusService=ai.openclaw.ClawRunner
X-KDE-Runner-DBusPath=/runner

# Trigger (shown in KRunner help)
X-KDE-Runner-Syntaxes=claw : Clawdbot actions
